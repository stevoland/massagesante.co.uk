    </head>
    <body>
        <div class="bg"></div>
        <div class="padding">
            <div class="container">
                <header role="banner">
                    <div class="ribbon">
                        <h1 class="ribbon--heading">
                            <span>M</span>assage <span>S</span>anté
                        </h1>
                    </div>
                    <h2 class="tagline">Profesional mobile massage<span class="in-central-and-west-london"> in<span class="central-and-west"> Central<span class="and-west"> &amp; West</span></span> London</span></h2>

                    <div class="details">
                        <a href="tel:+447511360132">t: 07511 360132</a>
                        <a href="mailto:hello@massagesante.co.uk">e: hello@massagesante.co.uk</a>
                    </div>
                </header>

                <div class="main" id="content">
                                {{>content}}
                </div>

                <footer>
                    &copy; Copyright 2013 Massage Santé
                </footer>
            </div>
        </div>

        <script>
            if (document.getElementById) {
                document.documentElement.className = document.documentElement.className.split('no-js').join('js');
            }

            (function (w, d) {
                if (!d.getElementById) {
                    return;
                }

                function dtClickHandler (e) {
                    var dt = e.target,
                        dd;

                    if (dt.nodeName.toUpperCase() !== 'DT') {
                        return;
                    }

                    dd = dt.nextSibling;
                    while (dd && !(dd.nodeType === 1 && dd.nodeName.toUpperCase() === 'DD')) {
                        dd = dd.nextSibling;
                    }
                    if (dd.nodeName.toUpperCase() !== 'DD') {
                        return;
                    }

                    if (!/on/.test(dt.className)) {
                        dt.className += ' on';
                        dd.className += ' on';
                    } else {
                        dt.className = dt.className.split(' on').join('');
                        dd.className = dd.className.split(' on').join('');
                    }
                }

                var services = d.getElementById('services');
                if (services) {
                    services.addEventListener('click', dtClickHandler);
                }


                w.addEventListener("hashchange", function(event) {
                    var element = d.getElementById(location.hash.substring(1));

                    if (element) {
                        if (!/^(?:a|select|input|button|textarea)$/i.test(element.tagName)) {
                            element.tabIndex = -1;
                        }
                        element.focus();
                    }

                }, false);
            }(window, document));


            var _gaq=[["_setAccount","UA-38751991-1"],["_trackPageview"]];
            (function(d,t){var g=d.createElement(t),s=d.getElementsByTagName(t)[0];g.async=1;
            g.src=("https:"==location.protocol?"//ssl":"//www")+".google-analytics.com/ga.js";
            s.parentNode.insertBefore(g,s)}(document,"script"));
        </script>
    </body>
